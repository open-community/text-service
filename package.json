{
  "name": "@open-comunity/text-service",
  "version": "0.1.0",
  "description": "Starter kit for NodeJS",
  "author": "Abraham TEWA",
  "license": "MIT",
  "homepage": "https://github.com/open-community/text-service",
  "bugs": "https://github.com/open-community/text-service/issues",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "doc:jsdoc": "jsdoc -c jsdoc.config.json --readme README.md",
    "lint": "run-s lint:*",
    "lint:javascript": "eslint .",
    "lint:markdown": "remark . --quiet --frail",
    "semantic-release": "semantic-release",
    "start": "node dist/cli.js start",
    "start:src": "node --require @babel/register --require source-map-support src/cli.js start",
    "start:brk": "node --inspect-brk --require @babel/register src/cli.js",
    "test": "gulp clean:report:tests:unit && mocha src/**/*.spec.js --opts mocha.opts",
    "test-cov": "gulp clean:report:tests:unit && cross-env NODE_ENV=test nyc mocha src/**/*.spec.js --opts mocha.opts",
    "test:integration": "mocha tests/integration/specs/**/*.js --opts tests/integration/setup/mocha.opts",
    "test:integration-cov": "gulp clean:report:tests:integration && mocha tests/integration/specs/**/*.js --opts tests/integration/setup/mocha.opts",
    "update:reference": "jsdoc2md -c jsdoc.config.json --files src > docs/REFERENCE.md"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-community/account-service.git"
  },
  "dependencies": {
    "@open-community/service-tools": "git@github.com:open-community/service-tools.git#develop",
    "base64url": "^3.0.1",
    "better-console": "^1.0.1",
    "body-parser": "^1.19.0",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.0.0",
    "elasticsearch": "^16.3.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@open-community/faker": "git@github.com:open-community/faker",
    "@open-community/internal-client": "git@github.com:open-community/internal-client-js.git#develop",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "async": "^3.1.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-jest-snapshot": "^2.0.0",
    "cross-env": "^5.2.0",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "jsdoc": "^3.6.3",
    "jsdoc-oblivion": "0.0.9",
    "minami": "^1.1.1",
    "mocha": "^6.2.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.1",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "semantic-release": "^15.13.19",
    "sinon": "^7.4.1",
    "source-map-support": "^0.5.13"
  }
}
